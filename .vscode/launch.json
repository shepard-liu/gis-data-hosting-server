{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}\\server",
            "program": "${workspaceFolder}\\server\\bin\\www.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
    ]
}